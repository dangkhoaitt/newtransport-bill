{
    "name": "newstransport-cms",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "copy": "cp src/login.html dist/login.html && cp src/server.js dist/server.js",
        "build": "yarn run clean && webpack -p --config=configs/webpack/prod.js && yarn run copy",
        "clean": "rimraf dist",
        "dev": "yarn run start-dev",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts*\" --fix",
        "start": "node dist/server.js",
        "start-dev": "webpack-dev-server --config=configs/webpack/dev.js ",
        "test": "jest --coverage --watchAll --config=configs/jest.json",
        "server:dev": "nodemon dist/server.js"
    },
    "husky": {
        "hooks": {
            "pre-commit": "yarn run lint"
        }
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "dependencies": {
